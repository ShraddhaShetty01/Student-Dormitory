<% include partials/header %>
<section id="header" class="jumbotron text-center">
            <h1 class="display-4 ">FIND YOUR HAPPY PLACE</h1>
            <p> A Top equipped place for students and young professionals</p>
        </section>
        <section id="gallery">
            <div class="container">
                 <div class="row">
                     
                     <% dorm.Room.forEach(function(room){%>
                     
                     <div class="col-lg-4 mb-4 ">
                   
                         <div class="card">
                             <img class="card-img-top" src="<%= room.roomimage%>" height="250" width="230">
                             <div class="card-body">
                                 <h5 class="card-title" color="black" ><%= room.roomtype%></h5>
                                 <p class="card-text"> 14.14/15.46 meter square</p>
                                 <a href="" class="btn btn-outline-success btn-sm">Select</a>
                                 <a href="/dorms" class="btn btn-outline-success btn-sm">Return</a>
                                 <a href="" class="btn btn-outline-danger btn-sm"><i class="far fa-heart"></i></a>
                             </div>
                         </div>
                     </div>
                     
                     <% });%>
               
                 </div>
            </div>
         </section>
<% include partials/footer %>